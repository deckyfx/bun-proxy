{
  "name": "bun-proxy",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --hot run src/index.ts",
    "drizzle": "bunx drizzle-kit generate --dialect sqlite --schema ./src/db/schema/index.ts --out ./src/db/drizzle",
    "migrate": "bun run src/db/migrations/index.ts",
    "seed": "bun run src/db/seeds/index.ts",
    "setup-db": "bun run drizzle && bun run migrate && bun run seed"
  },
  "dependencies": {
    "drizzle-orm": "^0.43.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/jsonwebtoken": "^9.0.9",
    "drizzle-kit": "^0.31.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
