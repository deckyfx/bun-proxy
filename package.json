{
  "name": "bun-proxy",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --hot run src/index.ts",
    "build:tailwind": "bunx @tailwindcss/cli -i ./src/app/assets/styles/styles.css -o ./src/app/assets/styles/tailwind.css",
    "drizzle": "bunx drizzle-kit generate --dialect sqlite --schema ./src/db/schema/index.ts --out ./src/db/drizzle",
    "migrate": "bun run src/db/migrations/index.ts",
    "seed": "bun run src/db/seeds/index.ts",
    "setup-db": "bun run drizzle && bun run migrate && bun run seed",
    "test:hit": "bun run test-dns.ts",
    "test:sse": "bun run test-sse.ts",
    "test:serverhit": "bun dev > ./server.log 2>&1 & DEV_PID=$!; bun test:hit > ./client.log 2>&1; kill $DEV_PID"
  },
  "dependencies": {
    "@types/dns2": "^2.0.9",
    "dns-packet": "^5.6.1",
    "dns2": "^2.1.0",
    "drizzle-orm": "^0.43.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "uuid": "^11.1.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.8",
    "@types/bun": "latest",
    "@types/dns-packet": "^5.6.5",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.1",
    "tailwindcss": "^4.1.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
